<h1>Cat Request Information</h1>

<table>
  <colgroup span="4"></colgroup>
  <tr>
    <th>Name</th>
    <th>ID</th>
    <th>Start date</th>
    <th>End Date</th>
    <th>Status</th>
  </tr>

  <% cat_name = Cat.find_by(id: @cat_rental_request.cat_id).name %>
  <tr>
    <td><%= cat_name %></td>
    <td><%= @cat_rental_request.cat_id %></td>
    <td><%= @cat_rental_request.start_date %></td>
    <td><%= @cat_rental_request.end_date %></td>
    <td><%= @cat_rental_request.status %></td>
  </tr>

</table>
