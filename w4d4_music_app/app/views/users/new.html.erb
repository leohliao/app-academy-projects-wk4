<h1>New User</h1>

<%= link_to "Sign In", new_session_url %>

<form action=" <%= user_url %>" method="post">
  <input type="hidden"
         name="authenticity_token"
         value=" <%= form_authenticity_token %>">
  <p>
    <label>Email
      <input type="text" name="user[email]" >
    </label>
  </p>

  <p>
    <label for="">Password
      <input type="password" 
             name="user[password]">
    </label>
  </p>

    <input type="submit" value="Create User">
  </form>
